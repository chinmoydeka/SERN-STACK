{
  "name": "sern-stack",
  "version": "2.2.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@david.kucsai/react-pdf-table": "^0.3.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@react-pdf/renderer": "^1.6.12",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "archiver-zip-encryptable": "^1.0.7",
    "archiver-zip-encrypted": "^1.0.8",
    "aws-sdk": "^2.814.0",
    "bluebird": "^3.7.2",
    "browser-image-compression": "^1.0.13",
    "date-fns": "^2.16.1",
    "electron-is-dev": "^1.2.0",
    "keytar": "^7.4.0",
    "material-ui-icons": "^1.0.0-beta.36",
    "node-pre-gyp": "^0.17.0",
    "react": "^17.0.1",
    "react-detect-offline": "^2.4.3",
    "react-dom": "^17.0.1",
    "react-json-parser": "^0.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "reactjs-popup": "^2.0.4",
    "sqlite3": "^5.0.0",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "electron-dev": "concurrently \" yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder build --windows -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "wait-on": "^5.2.1"
  },
  "author": {
    "name": "chinmoy deka",
    "email": "chinmoydeka556@gmail.com",
    "url": "https://gharnetwork.com"
  },
  "build": {
    "appId": "com.test.sern-stack",
    "productName": "sern-stack",
    "copyright": "Copyright Â© 2021 ${author}",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "db/**/*"
    ],
    "extraResources": [
      {
        "from": "./db/",
        "to": "extraResources",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
